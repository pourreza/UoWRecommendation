  <script>//import the target fasta
        String[] gi_array = gi_val.split(System.getProperty("line.separator"));
	String [] target_array = tfasta_in.split("\n");

	List targets = new ArrayList();
	List blast_gis = new ArrayList();

        String fastas = "";
        String value = "";

	for (int i =0; i&lt;target_array.length; i++)
	{
		String line = target_array[i];
		if (line.startsWith("&gt;"))
		{
			targets.add(line);
		}
	}

	for (int i=0; i&lt;gi_array.length; i++)
	{
		blast_gis.add(gi_array[i]);
	}

	for (Object o: targets)
	{
		String target = o.toString();
		if (!blast_gis.contains(target))
		{
			fastas += "\n"+target;	
		}
	}
	//export proteins in fasta format
	cfasta_out = fastas.trim();</script>
